Host *
    AddKeysToAgent yes
    UseKeychain yes
    IdentityFile ~/.ssh/id_ed25519

Host myspark
    HostName 192.168.1.93
    User username

Host myspark-ts
    HostName 100.86.102.108
    User username

Host *.trycloudflare.com
    HostName %h
    User root
    Port 22
    ProxyCommand /opt/homebrew/bin/cloudflared access ssh --hostname %h
